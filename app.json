{
  "expo": {
    "name": "Aspectus",
    "slug": "Aspectus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "infoPlist": {
        "NSRemindersUsageDescription": "Calendar app requires calendar integration"
      },
      "supportsTablet": true
    },
    "android": {
      "permissions": {
        "READ_CALENDAR": true,
        "WRITE_CALENDAR": true
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
